<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="eu.europeana.uim.logging.database">

    <query name="eu.europeana.uim.logging.database.TStringDatabaseLogEntry.findByExecution">
        SELECT s
        FROM TStringDatabaseLogEntry s
        WHERE s.executionId = :executionId
        <query-param name="executionId" type="java.lang.Long" />
    </query>

    <query name="eu.europeana.uim.logging.database.TObjectDatabaseLogEntry.findByExecution">
        SELECT o
        FROM TObjectDatabaseLogEntry o
        WHERE o.executionId = :executionId
        <query-param name="executionId" type="java.lang.Long" />
    </query>

    <query name="eu.europeana.uim.logging.database.TDurationDatabaseEntry.findByPlugin">
        SELECT d
        FROM TDurationDatabaseEntry d
        WHERE d.pluginName = :pluginName
        <query-param name="pluginName" type="java.lang.String" />
    </query>

</hibernate-mapping>