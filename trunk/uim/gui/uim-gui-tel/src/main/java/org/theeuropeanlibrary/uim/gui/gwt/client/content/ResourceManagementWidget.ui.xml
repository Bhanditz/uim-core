<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:c='urn:import:com.google.gwt.user.cellview.client' xmlns:s="urn:import:org.theeuropeanlibrary.uim.gui.gwt.client.content">

    <ui:style>
        .splitpanel {
        	height: 400px;
        	width: 100%;
        	min-width: 500px;
        	border: 4px solid #d0e4f6;
        }
        
        .executionFormCell {
        	padding-top: 20px;
        }
   
        .leftPanel {
        	background-color: #FFF;
        }
        
        .centerPanel {
        	background-color: #FFF;
        }
        
        .rightPanel {
        	background-color: #FFF;
        }
    </ui:style>

    <g:HTMLPanel>
        <table cellspacing='0' cellpadding='0' style='width:100%;'>
            <tr>
                <td valign='top'>
                    <g:SplitLayoutPanel ui:field="splitPanel" addStyleNames="{style.splitpanel}">
                        <g:west size="500">
                            <g:LayoutPanel ui:field="leftPanel" addStyleNames="{style.leftPanel}">
                                <!-- <c:CellBrowser ui:field="cellBrowser" /> -->
                            </g:LayoutPanel>
                        </g:west>
                        <g:center>
                            <g:LayoutPanel ui:field="centerPanel" addStyleNames="{style.centerPanel}">
                                <!-- <c:CellTable ui:field="cellTable" /> -->
                            </g:LayoutPanel>
                        </g:center>
                    </g:SplitLayoutPanel>
                </td>
            </tr>
        </table>
    </g:HTMLPanel>
</ui:UiBinder>
